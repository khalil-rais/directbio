function direktbio_views_post_execute($view) {

  if ($view->id() == 'seeds_catalog' or $view->id() == 'popular_product'){
	$product_list = array();
	$total = count($view->result);
	$removed = 0;
	$view_result = $view->result;
	foreach ($view_result as $key => $value) {
		$product_id = $view_result[$key]->_entity->getProductId();
		$price = preg_split("/ /", $view_result[$key]->_entity->getPrice());
		$price = $price[0];
		if (!isset($product_list[$product_id])){
			$product_list[$product_id] = array("price"=>$price,"index"=>$key);
		}
		else{
			if ($product_list[$product_id]["price"]>$price){
				unset ($view_result[$product_list[$product_id]["index"]]);
				$product_list[$product_id] = array("price"=>$price,"index"=>$key);
				$removed ++;
			}
			else{
				unset ($view_result[$key]);
				$removed ++;
			}
		}		//dpm($view->result[$key]->_entity->getProductId().":".$view->result[$key]->_entity->getPrice());
	}
	// Adjust the results, count & pager of the view.
	/*$view->total_rows = $total - $removed;
	$view->pager->total_items = $total - $removed;*/
	$view->pager->updatePageInfo();
	$view->result = $view_result;

  }
}


function direktbio_form_alter(&$form, FormStateInterface &$form_state, $form_id) {
  // Alter login form and add own custom submit handler.
  //\Drupal::logger('parfum')->notice('The form id is:'.$form_id);	
  $matches = array();
  preg_match('/(commerce_order_item_add_to_cart_form_commerce_product_)|(views_form_commerce_cart_form_default)|(commerce_checkout_flow_multistep_default)|(views_exposed_form)/' , $form_id, $matches);
  
  if (in_array('commerce_order_item_add_to_cart_form_commerce_product_',$matches)) {
	$form['actions']['submit']['#submit'][] = '_direktbio_redirect_to_catalog_submit';
	if (in_array('commerce_order_item_add_to_cart_form_commerce_product_',$matches)){
		$session = \Drupal::request()->getSession();
		if (empty($session->get('my_http_referer')) and isset ($_SERVER ["HTTP_REFERER"])) {
			$session->set('my_http_referer', $_SERVER ["HTTP_REFERER"]);
			\Drupal::logger('parfum')->notice('Referrer is set to:'.$_SERVER ["HTTP_REFERER"]);
		}
		else{
			$exist = $session->get('my_http_referer');
			if (preg_match('/\/product\//',$exist)){
				$session->remove('my_http_referer');			
			}
		}
	}
  }
  elseif (in_array('views_form_commerce_cart_form_default',$matches)) {
		$options = ['attributes' => ['target' => '_blank']]; 
		$internal_link_2381 = Drupal\Core\Link::createFromRoute(t(" Terms of Sale"), 'entity.node.canonical', ['node' => 8],$options);
	  
		$form['terms'] = array(
			'#type' => 'checkbox',
			'#title' => t("I accept the").$internal_link_2381->toString(),
			'#required' => TRUE,
		);  
	  $form['#validate'][] = '_direktbio_last_check';
	  //dpm($form);
  }
}

/**
 * Custom submit handler for login form.
 */
function _direktbio_redirect_to_catalog_submit($form, FormStateInterface $form_state) {
	$session = \Drupal::request()->getSession();    
	$HTTP_REFERER = $session->get('my_http_referer');
	if (!empty ($HTTP_REFERER)){

		$url_table = parse_url($HTTP_REFERER);
		$url_parts =  preg_replace('/^\/en|^\/fr/', '', $url_table['path']);
			
		$form_state->setRedirectUrl(Url::fromUri('internal:'.$url_parts));

			
		$session->remove('my_http_referer');
	}
}

	
